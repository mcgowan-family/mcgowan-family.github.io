   background-color: #fff;
   color: #000;
}
/* General layout helper classes */
div.containerJHX .jhxAlignLeft {
   text-align: left;
}
div.containerJHX .jhxAlignCenter {
   text-align: center;
}
div.containerJHX .jhxAlignRight {
   text-align: right;
}
div.containerJHX .jhxClear {
   clear: both;
}

/* wrappers, containers, etc. */
div.containerJHX div.jhxContentWrapper { /* fdContentWrapper */
   padding: 1px 10px;
}
div.containerJHX div.panel,
div.containerJHX div.buttons { /* fdContentWrapper */
   padding: 1px 10px;
}
div.containerJHX div.box {
   padding: 5px 10px;
   margin: 0 0 10px 0;
   background-color: #eee;
   border-top: 1px solid #777;
   border-bottom: 1px solid #777;
   overflow: auto;
}
div.containerJHX table {
   width: 100%;
   border-top: 1px solid #777;
}
div.containerJHX table tr.zebra {
   background-color: #eee;
}
div.containerJHX table th {
   padding: 5px;
   background-color: #eee;
   border-bottom: 1px solid #777;
}
div.containerJHX table td {
   padding: 5px;
}
div.containerJHX table th {
   border-bottom: #fff 3px solid; 
   color:#fff;
   background:url("images/th_gradient_background.gif") repeat;
   background-color: #848687;
   padding: 5px 5px; 
   text-align: left;
}
div.containerJHX div.jhxSplitTwo { /* fdNAvSplitTwo */
   width: 50%;
   float: left;
   font-size: 0.6em;
   /* MAC IE Filter - macX bold bug \*/
   font-size: 100%;
   /* */
}
div.containerJHX ul.no-bullet {
   list-style-type: none;
   -moz-padding-start: 0;
   margin-top: 0;
}
div.containerJHX ul.no-bullet li {
   margin-left: 0;
}

/* Headers, etc. */
div.containerJHX h1 {
   padding: 5px 0 10px 0;
}
div.containerJHX h2 {
   font-size: 130%;
}
div.containerJHX div.heading { /* headingBar tag */
   border-bottom: solid 3px #fff;
   background: url("images/th_gradient_background.gif") repeat;
   background-color: #848687;
   color: #fff;
   height: 23px;
   padding: 5px 10px;
}
div.containerJHX div.heading h3, 
div.containerJHX div.heading div { /* headingBar tag content */
   color: #fff;
   width: 50%;
   float: left;
   display: inline-block;
   font-weight: bold;
   font-size: 0.6em;
   /* MAC IE Filter - macX bold bug \*/
   font-size: 100%;
   /* */
}
div.containerJHX div.heading div { /* headingBar tag right content */
   margin-top: 4px;
   text-align: right;
   float: right;
}
div.containerJHX div.panel div.heading h3,
div.containerJHX div.panel div.heading { /* undo some headingBar styles for panel headings (they use same CSS class name) */
   padding: 0px;
   border: 0;
}
div.containerJHX div.panel div.heading h3, 
div.containerJHX div.jhxPageNumbers h3 { /* panel tag headings */
   width: auto;
   display: block;
   background-image: none;
   background-color: #fff;
   font-size: 140%; 
   color: #000;
   /* Mac IE filter - os9 prob due to bde page code \*/
   padding-top:5px;
   padding-bottom:10px;
   padding-left:0px; /* DEMO made 0px for vertical alignment with other parts of the page */
   /* */
   font-weight: 100;
   margin:0px;
}
div.containerJHX div.jhxPageNumbers div {
   float: right;
   text-align: right;
   font-weight: bold;
   margin-top: 10px;
}

/* Stuff inside a formLayout tag (table.axis) */
div.containerJHX fieldset {
   margin: 0;
   padding: 0;
   border: 0;
}
div.containerJHX table.axis {
   width: 100%;
   border: 0;
   margin-bottom: 0;
}
div.containerJHX table.axis td,
div.containerJHX table.jhxCols-4 td {
   width: 60%;
   padding: 4px;
}
div.containerJHX table.axis td.label,
div.containerJHX table.jhxCols-4 td.label {
   width: 40%;
   padding-left: 0;
   vertical-align: top;
}
div.containerJHX table.jhxCols-4 td {
   width: 30%;
}
div.containerJHX table.jhxCols-4 td.label {
   width: 20%;
}

/* buttons and links */
div.containerJHX div.buttons { /* fdNavContainer */
   padding-top: 20px;
   padding-bottom: 20px;
   clear: both;
   height: 10px;
   background-color: #fff;
}
div.containerJHX a.act,
div.containerJHX a.jhxActionLink,
div.containerJHX a.jhxActionBack,
div.containerJHX a.jhxButtonPrint { /* fdActionLink */
   background: url("images/arrow2_right_link_normaltext.gif") left top no-repeat;
   padding-left: 15px;
   text-decoration: none;
   font-weight: bold;
   padding-right: 20px;
   text-decoration: underline;
   display: inline-block;
}
div.containerJHX a.jhxActionBack {
   background: url("images/arrow2_left_link_normaltext.gif") left top no-repeat;
}
div.containerJHX a.jhxNonBold {
   font-weight: normal;
}
div.containerJHX span.jhxLozengeWrapper { /* fdLozengeLinkTwo */
   padding-left: 14px;
   height: 25px;
   background: url("images/cta_lozenge_left_g.gif") no-repeat top left;
   display: inline-block;
}
div.containerJHX span.jhxLozengeWrapper span { /* fdLozengeLinkTwo */
   padding: 5px 25px 1px 3px;
   background: url("images/cta_lozenge_right_g.gif") no-repeat top right;
   float: left;
   height: 25px;
}
div.containerJHX span.jhxLozengeWrapper span a,
div.containerJHX span.jhxLozengeWrapper span input { /* fdLozengeLinkTwo */
   color: #fff;
   text-decoration: none;
   background-color: transparent;
   background-image: none;
   font-weight: bold;
   /* Added for JHX actionButton */
   border: 0;
   cursor: pointer;
   padding: 0 0 10px 0;
   height: 25px;
   overflow: visible; /* needed for IE only to make padding work */
   font-family: Arial, Helvetica, sans-serif;
}
div.containerJHX span.jhxLozengeWrapper span a:hover,
div.containerJHX span.jhxLozengeWrapper span input:hover { /* fdLozengeLinkTwo */
   color: #a7a7a7;
   background-color: transparent;
}
div.containerJHX span.jhxLozengeWrapper span.button-wrap {
   padding: 4px 22px 0 0;
}
div.containerJHX p.page-numbers {
   margin: 0;
   text-align: right;
   font-weight: bold;
}
div.containerJHX p.page-numbers a,
div.containerJHX p.page-numbers em {
   margin: 0 3px;
   display: inline-block;
   overflow: hidden;
}
div.containerJHX p.page-numbers em {
   font-style: normal;
   color: #777;
}

/* Images */
div.containerJHX img {
   border: 0;
}
div.containerJHX a img {
   vertical-align: top;
}

/* Field level errors */
div.containerJHX table.axis td > div ul {
   padding-left: 0px;
   list-style: none;
   margin: 0;
}
div.containerJHX table.axis td > div ul > li {
   color: #f00;
}
div.containerJHX table.axis td > div ul > li > strong,
div.containerJHX table.axis td > div ul > li > em {
   display: none; /* Not required? Should be removed by product - but what if it isn't? */
}

/* Page level errors */
div.containerJHX div.error {
   background-color: #fff;
   color: #f00;
   border: 0;
}
div.containerJHX div.error ul {
   padding: 0 0 0 20px;
   margin: 0;
}
div.containerJHX div.error ul li strong,
div.containerJHX div.error ul li em { /* the text "ERROR!" */
   display: none;
}
/* Page level errors inside a formlayout */
div.containerJHX table.axis div.error {
   padding: 0;
   margin: 0;
}
