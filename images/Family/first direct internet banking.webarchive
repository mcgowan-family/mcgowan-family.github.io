	color:#434343;
}

.fdCalcResultsTable {
	border-top:1px solid #a7a7a7;
	color:000;
}


.fdCalcResultsTable thead th, .fdCalcResultsTable thead td {
	background-color:#ebebeb;
	color:#434343;
	padding:3px 3px 3px 6px;
	vertical-align:top;
	text-align:left;
	border-left:dotted #a7a7a7 1px;
}

.fdCalcResultsTable tbody th {
	padding:3px 3px 3px 6px;
	font-weight:normal;
	text-align:left;
}

.fdCalcResultsTable tbody td {
	padding:3px 3px 3px 6px;
	border-left:dotted #a7a7a7 1px;
}

.fdCalcResultsTable tbody td.standard {
	border-left:none;
}

.fdCalcResultsTable tbody td.infoHighlight {
	padding:10px 6px;;
	background-color:#ebebeb;
	font-weight:bold;
}

.fdCalcResultsTable thead th.first, .fdCalcResultsTable thead td.first, .fdCalcResultsTable tbody th.first, .fdCalcResultsTable tbody td.first {
	border-left:none;
	padding-left:0px;

}

div.fdFormSectionSeparator {
	background:url(images/form_separator.gif) repeat-x 0% 50%;
	clear:both;
	text-align:center;
}

div.impNote {
	background:url(images/impnote.gif) no-repeat top left;
	padding-left:35px;
}

.fdFormSectionSeparatorText {
	background:url(images/form_separator.gif) repeat-x 0% 50%;
	clear:both;
	text-align:center;
}

.separatorLarge {
	margin: 15px 0px;
}

td.fdFormSectionSeparator {
	border-top:solid 1px #a7a7a7;
}

.fdFormSectionSeparatorText span {
	background-color:#f2f2f2;
	padding:0px 3px;
}

.fdFormTable {
	clear:both;
	margin:0px 10px;
}

.fdFormTable td {
	padding:8px 10px 8px 0px;
	vertical-align:top;
}

.fdFormTable th {
	text-align:left;
}

div.reqFieldText {
	float:right;
	padding-top:6px;
	font-size:90%;
	text-align:right;
}

div.fdFormNoteHighlight {
	background-color:#ebebeb;
	margin-top:10px;
	clear:both;
}

div.fdFormNoteHighlight:lang(en) {
	margin-top:30px;
}

div.fdFormNoteHighlight ul {
	padding:10px 10px 10px 18px;
	margin:0px;
}


div.fdFormNoteHighlight ul li {
	font-weight:bold;
	list-style-type:none;
	background: url("images/arrow_form_impnote.gif") no-repeat top left;
	padding: 0px 0px 10px 16px;
}

.clearCol {
	clear:both;
}

table.fdFormSummaryTableOne {
	width:550px;
}

table.fdFormSummaryTableOne th, table.fdFormSummaryTableOne td {
	text-align:left;
}

table.fdFormSummaryTableOne th.last, table.fdFormSummaryTableOne td.last {
	text-align:right;
}

table.fdFormSummaryTableTwo {
	width:590px;
}

table.fdFormSummaryTableTwo td {
