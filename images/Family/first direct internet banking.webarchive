	text-align:right;
	vertical-align:top;
	font-weight:bold;
}

table.fdFormSummaryTableTwo th {
	padding:2px 0px 2px 0px;
	border-bottom: solid 1px #ebebeb;
	text-align:right;
	vertical-align:top;
	font-weight:normal;
}


table.fdFormSummaryTableTwo tr.highlight {
	background-color:#ebebeb;
}

table.fdFormSummaryTableTwo tr.highlight td{
	font-size:120%;
	font-weight:bold;
	padding:10px 30px;
	border-bottom:none;
}

table.fdFormSummaryTableTwo tr.highlight th{
	font-size:120%;
	font-weight:bold;
	padding:2px 0px 2px 0px;
	border-bottom:none;
	vertical-align:middle;
}

div.fdFormSignature {
	border:1px inset #434343; 
	height: 340px; 
	background-color:#fff; 
	overflow: auto;
	padding:5px;
}

div.fdFormSignature h3, div.fdFormSignature h4 {
	font-size:100%;
	color:#434343;
}

div.fdFormSignature ol li ul {
	padding-left:12px;
	list-style-type:disc;
}

div.fdFormSignature div.box {
	border:solid 1px #a7a7a7;
	padding:5px;
}

div.fdFormSignature div.multiple {
	border-color:#a7a7a7;
	border-width: 0px 1px 1px 1px;
	border-style:solid;
